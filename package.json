{
  "name": "fixtweet",
  "version": "1.0.0",
  "description": "Embed Twitter videos, polls, and more on Discord and Telegram",
  "main": "dist/worker.js",
  "scripts": {
    "build": "node esbuild.config.mjs",
    "publish": "wrangler deploy --no-bundle",
